    "dotenv": "^8.2.0",
    "fuse.js": "^6.4.6",
    "is-reachable": "^5.0.0",
    "node-cron": "^2.0.3",
    "telegraf": "^3.38.0",
    "xeorarch": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-typescript": "^7.12.1",
    "@telegraf/core": "^1.1.0",
    "@types/eslint": "^7.2.4",
    "@types/node": "^14.14.6",
